{
    "name": "stepanenko3/nova-filemanager",
    "description": "Filemanager tool for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "filemanager"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^4.0",
        "stepanenko3/laravel-helpers": "*",
        "stepanenko3/laravel-pagination": "*"
    },
    "autoload": {
        "psr-4": {
            "Stepanenko3\\NovaFilemanager\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stepanenko3\\NovaFilemanager\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
